using System;
using System.Collections.Generic;
using System.Text;

namespace CSharpBasic
{
    public class AccessModifiers1
    {
        private int _ID;
        protected string _lastname;
       public int ID { get; set; }
    }

    public class AccessModifiersclass2 : AccessModifiers1
    {
       public void PrintID()
        {
            AccessModifiersclass2 AID = new AccessModifiersclass2();
           // AID._id = 101;
            AID._lastname = "Meenal";
            Console.WriteLine("Name {0}", AID._lastname);
            base._lastname = "Juhi";

        }
    }
}
